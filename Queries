TABLE


CREATE TABLE CRIME_TB (
    CASEID VARCHAR2(12) PRIMARY KEY,
    CASETITLE VARCHAR2(80) NOT NULL,
    SUSPECTNAME VARCHAR2(50) NOT NULL,
    CRIMETYPE VARCHAR2(30) NOT NULL,
    CRIMEDATE DATE NOT NULL,
    LOCATION VARCHAR2(60) NOT NULL,
    REMARKS VARCHAR2(100)
);



SEQUENCE


CREATE SEQUENCE CRIME_SEQ
START WITH 10
INCREMENT BY 1
MAXVALUE 999999
NOCYCLE;


